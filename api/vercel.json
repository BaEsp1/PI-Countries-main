{
  "functions": {
    "api/**/*.js": {
      "runtime": "vercel@2",
      "includeFiles": "api/**/*"
    },
    "api/node_modules/pg/**/*": {}
  },
  "public": false,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}